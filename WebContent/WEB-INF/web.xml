<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>LeaveRequest</display-name>
  <servlet>
    <servlet-name>LoadProps</servlet-name>
    <servlet-class>com.backyardev.LoadPropsServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Leave</servlet-name>
    <servlet-class>com.backyardev.LeaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Leave</servlet-name>
    <url-pattern>/leave</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Comp</servlet-name>
    <servlet-class>com.backyardev.CompServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Comp</servlet-name>
    <url-pattern>/comp</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CompOff</servlet-name>
    <servlet-class>com.backyardev.CompOffServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompOff</servlet-name>
    <url-pattern>/compoff</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>LeaveDisplayServlet</servlet-name>
    <servlet-class>com.backyardev.LeaveDisplayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveDisplayServlet</servlet-name>
    <url-pattern>/leave/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.backyardev.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Portal</servlet-name>
    <servlet-class>com.backyardev.PortalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Portal</servlet-name>
    <url-pattern>/portal</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Setting</servlet-name>
    <servlet-class>com.backyardev.SettingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Setting</servlet-name>
    <url-pattern>/setting</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.backyardev.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Action</servlet-name>
    <servlet-class>com.backyardev.ActionLeaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Action</servlet-name>
    <url-pattern>/action</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/WEB-INF/login.jsp</welcome-file>
  </welcome-file-list>

</web-app>